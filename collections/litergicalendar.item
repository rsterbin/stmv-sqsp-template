{.section item}
<div class="row column subtitle">
    <h1>Liturgical Calendar: Service on {startDate|date %A, %B %e, %Y}</h1>
</div>

<div class="row">
    <div class="small-12 columns">
        <div class="liturgical-layout-full detail">
            <div class="liturgical-calendar-day{.repeated section tags} {@|url-encode}{.end}">
                <h3>{title}</h3>
                {body}
            </div>
        </div>
    </div>
    <div class="small-12 columns">
        <div class="liturgical-calendar-week" rel="{startDate|date %Y-%m-%d}">
            <div class="week-services"></div>
        </div>
    </div>
</div>
{.end}
